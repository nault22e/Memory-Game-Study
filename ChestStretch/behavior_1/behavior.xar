<?xml version="1.0" encoding="UTF-8" ?><ChoregrapheProject xmlns="http://www.aldebaran-robotics.com/schema/choregraphe/project.xsd" xar_version="3"><Box name="root" id="-1" localization="8" tooltip="Root box of Choregraphe&apos;s behavior. Highest level possible." x="0" y="0"><bitmap>media/images/box/root.png</bitmap><script language="4"><content><![CDATA[]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="3" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="4" /><Timeline enable="0"><BehaviorLayer name="behavior_layer1"><BehaviorKeyframe name="keyframe1" index="1"><Diagram><Box name="Facilitate Exercise" id="9" localization="8" tooltip="" x="680" y="235"><bitmap>media/images/box/box-python-script.png</bitmap><script language="4"><content><![CDATA[from naoqi import ALProxy
import random
from time import sleep
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        self.tts = ALProxy("ALTextToSpeech")

        try:
            # create proxy on ALMemory
            memProxy = ALProxy("ALMemory","localhost",9559)

        except RuntimeError,e:
            # catch exception
            print "error insert data", e

        exerLength = memProxy.getData("exLength")

        #Phrases of encouragement
        self.phrases = ["Good job", "Well done", "Keep up the great work", "Top job", "Amazing performance", "You can do this", "I’m so proud of you", "Awesome work", "You are a champion", "Keep trying, you are looking better already", "Terrific", "Way to go", "You rock", "I’m impressed", "Bravo", "Astounding", "You are invincible", "That was divine", "Superb", "Fantastic", "Splendid", "Remarkable", "Brilliant"]

        self.newSet = False
        self.motionComplete = False
        self.currSet = 1

        if(exerLength == "short"):
            self.onSay("We will do two sets, holding the stretch for 4 seconds ecah time. Ready,\\pau=1000\\ begin!")
            sleep(6.0)
            self.performShortEx()


        elif(exerLength == "long"):
            self.onSay("We will do one set, holding the stretch for 15 seconds. Ready,\\pau=1000\\ begin!")
            sleep(6.0)
            self.performLongEx()


    def performShortEx(self):
        if self.currSet == 2:
            self.onSay("Remember, sticking out your chest will deepen the stretch")
            sleep(3.0)

        self.startRep() #Bend down
        self.waitMvt() #Wait for movement to finish

        for j in range(1, 5): #Hold for 4 seconds
            self.say = str(j)
            self.tts.say(self.say)
            sleep(1.0)


        self.endRep() #Sit up
        self.waitMvt() #Wait for movement to finish


        if self.currSet == 1:
            self.onSay("Now we will take a break. When you are ready to begin the second and final set, touch my hand.")
            self.nextSet()
            while not self.newSet:
                pass
            self.newSet = False
            self.onSay("Let's begin")
            self.currSet += 1
            self.performShortEx()

        else:
            self.finishEx()

    def performLongEx(self):
        self.startRep() #Bend down
        self.waitMvt() #Wait for movement to finish

        self.onSay("Remember, sticking out your chest will deepen the stretch")
        sleep(5.0)

        for j in range(1, 16): #Hold for 15 seconds
            self.onSay(str(j))
            sleep(1.0)


        self.endRep() #Sit up
        self.waitMvt() #Wait for movement to finish

        self.finishEx()


    def finishEx(self):
        self.onSay("You have completed the chest stretch. " + random.choice(self.phrases))
        self.onStopped()

    def waitMvt(self):
        while self.motionComplete == False:
                pass
        self.motionComplete = False

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def onInput_onMvt(self):
        self.motionComplete = True

    def onInput_startSet(self):
        self.newSet = True]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="3" /><Input name="startSet" type="1" type_size="1" nature="1" inner="0" tooltip="" id="4" /><Input name="onMvt" type="1" type_size="1" nature="1" inner="0" tooltip="" id="5" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="6" /><Output name="startRep" type="1" type_size="1" nature="2" inner="0" tooltip="" id="7" /><Output name="nextSet" type="1" type_size="1" nature="2" inner="0" tooltip="" id="8" /><Output name="endRep" type="1" type_size="1" nature="2" inner="0" tooltip="" id="9" /><Output name="onSay" type="3" type_size="1" nature="2" inner="0" tooltip="" id="10" /></Box><Box name="Hands" id="11" localization="8" tooltip="Enter description here" x="695" y="18"><bitmap>media/images/box/interaction/social_behavior.png</bitmap><script language="4"><content><![CDATA[]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="Tactile L.Hand_onStart" type="1" type_size="1" nature="2" inner="0" tooltip="This input has been automatically generated&#x0A;by converting several boxes into a single box." id="2" /><Output name="TactileLHand_backTouched" type="1" type_size="1" nature="1" inner="0" tooltip="This output has been automatically generated&#x0A;by converting several boxes into a single box." id="3" /><Timeline enable="0"><BehaviorLayer name="behavior_layer1"><BehaviorKeyframe name="keyframe1" index="1"><Diagram><Box name="Tactile L.Hand" id="3" localiz